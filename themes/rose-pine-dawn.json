            "color": "#ea9d34"
          },
          "string.escape": {
            "color": "#b4637a"
          },
          "string.regex": {
            "color": "#56949f"
          },
          "string.special": {
            "color": "#b4637a"
          },
          "string.special.symbol": {
            "color": "#b4637a"
          },
          "tag": {
            "color": "#286983"
          },
          "text.literal": {
            "color": "#ea9d34"
          },
          "title": {
            "color": "#907aa9",
            "font_weight": 600
          },
          "type": {
            "color": "#56949f"
          },
          "property": {
            "color": "#907aa9"
          },
          "variable.special": {
            "color": "#907aa9"
          }
